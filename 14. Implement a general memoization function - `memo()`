function memo(func, resolver) {
  const map = new Map();
  return function (...args) {
    const key = resolver ? resolver(...args) : args.join("_");
    if (map.has(key)) {
      return map.get(key)
    } else {
      const res = func.call(this, ...args);
      map.set(key, res);
      return res;
    }
  }
}
